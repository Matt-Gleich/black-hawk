  "tokenColors": [
    {
      "name": "Red Italic",
      "scope": [
        "keyword",
        "meta.selector.css",
        "keyword.control.at-rule.media.css"
      ],
      "settings": {
        "foreground": "#e92741",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Red",
      "scope": ["entity.name.tag", "markup.heading"],
      "settings": {
        "foreground": "#e92741"
      }
    },
    {
      "name": "Deep Red",
      "scope": ["variable.other.property", "support.class"],
      "settings": {
        "foreground": "#d8466a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Yellow",
      "scope": ["string"],
      "settings": {
        "foreground": "#E1DB3F"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "constant.numeric",
        "entity.other.keyframe-offset.percentage.css"
      ],
      "settings": {
        "foreground": "#418EDD"
      }
    },
    {
      "name": "Green",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#3EC841"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "meta.template.expression",
        "meta.return-type",
        "support.type.primitive",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#ff9900"
      }
    },
    {
      "name": "Gray",
      "scope": ["comment"],
      "settings": {
        "foreground": "#343c50",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Teal",
      "scope": [
        "storage",
        "keyword.control.export",
        "support.class.math",
        "entity.other.attribute-name",
